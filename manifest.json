{

  "manifest_version": 2,
  "name": "Pinned Tab Fix",
  "version": "2.3.0",

  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/nralbrecht/pinned-tab-fix",
  "default_locale": "en",

  "applications": {
    "gecko": {
      "id": "@pined_tab_fix",
      "strict_min_version": "48.0"
    }
  },

  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },

  "background": {
    "scripts": ["background-script.js"]
  },

  "options_ui": {
      "page": "options/options.html"
  },

  "permissions": [
    "storage",
    "tabs"
  ]

}
